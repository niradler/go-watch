{
  "watch_dirs": [".", "bin"],
  "ignore_dirs": ["node_modules", ".git"],
  "rules": [
    {
      "extensions": ["json"],
      "command": "echo json file changed"
    },
    {
      "extensions": ["go"],
      "command": "go test -v"
    }
  ],
  "debounce_time": "3s",
  "live_reload": true,
  "live_reload_port": 35729
}